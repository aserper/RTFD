{
  "name": "rtfd-mcp",
  "version": "0.5.2",
  "description": "MCP server for real-time library documentation access across multiple package ecosystems (PyPI, npm, crates.io, GoDocs, DockerHub, GitHub, GCP, and more).",
  "author": {
    "name": "Amit Serper",
    "email": "aserper+noreply@github.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/aserper/RTFD",
  "repository": {
    "type": "git",
    "url": "https://github.com/aserper/RTFD.git"
  },
  "keywords": [
    "mcp",
    "documentation",
    "pypi",
    "npm",
    "crates",
    "godocs",
    "docker",
    "github",
    "gcp"
  ],
  "category": "documentation",
  "mcp_servers": [
    {
      "name": "rtfd",
      "command": "python",
      "args": [
        "-m",
        "RTFD.server"
      ],
      "env": {
        "RTFD_FETCH": "${RTFD_FETCH:true}",
        "VERIFIED_BY_PYPI": "${VERIFIED_BY_PYPI:false}",
        "GITHUB_AUTH": "${GITHUB_AUTH:false}",
        "GITHUB_TOKEN": "${GITHUB_TOKEN:}"
      }
    }
  ]
}
